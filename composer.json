{
    "name": "swooletw/hyperf-encryption",
    "type": "library",
    "description": "The encryption package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "encryption",
        "swoole"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert.cht@pm.me",
            "homepage": "https://albert-chen.com"
        }
    ],
    "support": {
        "issues": "https://github.com/swooletw/hyperf-packages/issues",
        "source": "https://github.com/swooletw/hyperf-packages"
    },
    "autoload": {
        "psr-4": {
            "SwooleTW\\Hyperf\\Encryption\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-hash": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "hyperf/config": "~3.1.0",
        "hyperf/tappable": "~3.1.0",
        "hyperf/stringable": "~3.1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "SwooleTW\\Hyperf\\Encryption\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "3.1-dev"
        }
    }
}