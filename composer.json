{
    "name": "laravel-hyperf/encryption",
    "type": "library",
    "description": "The encryption package for Laravel Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "encryption",
        "swoole",
        "laravel-hyperf"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@laravel-hyperf.com"
        }
    ],
    "support": {
        "issues": "https://github.com/laravel-hyperf/components/issues",
        "source": "https://github.com/laravel-hyperf/components"
    },
    "autoload": {
        "psr-4": {
            "LaravelHyperf\\Encryption\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "ext-hash": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "hyperf/config": "~3.1.0",
        "hyperf/tappable": "~3.1.0",
        "hyperf/stringable": "~3.1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "LaravelHyperf\\Encryption\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    }
}